public class Ball {

	private Coin coin;
	private int x, y;
	private boolean looted;
	
	public Ball(Coin coin, int x, int y, boolean looted) {
		this.coin = coin;
		this.x = x;
		this.y = y;
		this.looted = looted;
	}

	public boolean isLooted() {
		return looted;
	}

	public void setLooted(boolean looted) {
		this.looted = looted;
	}

	public Coin getCoin() {
		return coin;
	}

	public void setCoin(Coin coin) {
		this.coin = coin;
	}

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}

}
